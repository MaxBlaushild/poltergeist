# Flutter SDK location (already set by Flutter)
# flutter.sdk=/path/to/flutter

# Android SDK location (already set by Android Studio)
# sdk.dir=/path/to/android/sdk

# Google Maps API Key
google.maps.key=YOUR_GOOGLE_MAPS_API_KEY_HERE

